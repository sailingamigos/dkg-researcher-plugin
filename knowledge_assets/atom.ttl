@prefix : <http://example.org#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# Classes
:Entity rdf:type rdfs:Class .

# Properties

:title rdf:type rdf:Property ;
    rdfs:domain :Entity ;
    rdfs:range xsd:string .

:summary rdf:type rdf:Property ;
    rdfs:domain :Entity ;
    rdfs:range xsd:string .

:author rdf:type rdf:Property ;
    rdfs:domain :Entity ;
    rdfs:range xsd:string .

:link rdf:type rdf:Property ;
    rdfs:domain :Entity ;
    rdfs:range xsd:string .

:primary_category rdf:type rdf:Property ;
    rdfs:domain :Entity ;
    rdfs:range xsd:string .

:category rdf:type rdf:Property ;
    rdfs:domain :Entity ;
    rdfs:range xsd:string .

:updated rdf:type rdf:Property ;
    rdfs:domain :Entity ;
    rdfs:range xsd:dateTime .

:published rdf:type rdf:Property ;
    rdfs:domain :Entity ;
    rdfs:range xsd:dateTime .